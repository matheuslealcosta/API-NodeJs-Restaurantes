# ===== CONFIGURAÇÕES DO BANCO DE DADOS =====
DATABASE_URL="postgresql://postgres:postgres123@localhost:5432/restaurants_api?schema=public"

# ===== CONFIGURAÇÕES DE AUTENTICAÇÃO =====
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="1h"

# ===== CONFIGURAÇÕES DO SERVIDOR =====
NODE_ENV="development"
PORT=3000

# ===== CONFIGURAÇÕES DE CORS =====
ALLOWED_ORIGINS="http://localhost:3000,http://localhost:3001,http://localhost:8080"

# ===== CONFIGURAÇÕES DE LOG =====
LOG_LEVEL="info"

# ===== CONFIGURAÇÕES DO POSTGRESQL (para Docker) =====
POSTGRES_DB="restaurants_api"
POSTGRES_USER="postgres"
POSTGRES_PASSWORD="postgres123"

# ===== CONFIGURAÇÕES DO PGADMIN (para Docker) =====
PGADMIN_DEFAULT_EMAIL="admin@restaurants.com"
PGADMIN_DEFAULT_PASSWORD="admin123"
